        debugger
        let overflowContent = splitContent(editableTd);

        if (overflowContent) {
            createNewRow(overflowContent);
        }
    }

    // Function to split content if it exceeds 300px
    function splitContent(element) {
        debugger
        if (element.scrollHeight > 300) {
            let visibleHeight = 300;
            let content = element.textContent;
            let visibleContent = "";
            let overflowContent = "";

            element.textContent = "";

            for (let i = 0; i < content.length; i++) {
                element.textContent += content[i];
                if (element.scrollHeight > visibleHeight) {
                    overflowContent = content.slice(i);
                    visibleContent = content.slice(0, i);
                    break;
                }
            }
            debugger
            element.textContent = visibleContent;
            return overflowContent;
        }
        return null;
    }

